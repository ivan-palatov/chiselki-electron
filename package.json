{
  "name": "chiselki-electron",
  "version": "0.0.1",
  "license": "MIT",
  "author": {
    "name": "Ivan Palatov",
    "email": "ivanakazver3d@gmail.com",
    "url": "https://github.com/ZVER3D"
  },
  "description": "Calculating integrals and equations using mathmatical aproximations",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "build-32": "electron-builder --ia32",
    "build-64": "electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "electronWebpack": {
    "title": true,
    "renderer": {
      "webpackConfig": "webpack.renderer.js"
    },
    "whiteListedModules": [
      "@material-ui/core",
      "@material-ui/icons",
      "react-router-dom",
      "react-router",
      "formik",
      "mobx",
      "mobx-react-lite",
      "react-katex",
      "react-plotly.js"
    ]
  },
  "build": {
    "appId": "com.zver3d.numerical.methods",
    "productName": "Численные Методы",
    "copyright": "Copyright © 2019 Ivan Palatov",
    "win": {
      "target": "portable",
      "icon": "static/installerIcon.ico"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.5.1",
    "@types/katex": "^0.10.2",
    "css-baseline": "^1.1.10",
    "formik": "^2.0.4",
    "katex": "^0.11.1",
    "mathjs": "^6.2.3",
    "mobx": "^5.15.0",
    "mobx-react-lite": "^1.5.1",
    "node-worker-farm": "^1.3.1",
    "plotly.js": "^1.51.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-katex": "^2.0.2",
    "react-plotly.js": "^2.4.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.5.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.7.0",
    "@types/mathjs": "^6.0.2",
    "@types/node": "^12.12.8",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-plotly.js": "^2.2.4",
    "@types/react-router-dom": "^5.1.2",
    "@types/yup": "^0.26.24",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^3.2.0",
    "electron": "9.4.0",
    "electron-builder": "^22.1.0",
    "electron-webpack": "^2.7.4",
    "electron-webpack-ts": "^3.2.0",
    "file-loader": "^4.2.0",
    "prettier": "^1.19.1",
    "style-loader": "^1.0.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.2",
    "url-loader": "^2.2.0",
    "webpack": "~4.41.2",
    "webpack-merge": "^4.2.2",
    "worker-loader": "^2.0.0"
  }
}
